{
    "name": "lucasgiovanny/laravel-serpro-datavalid",
    "description": "Laravel package to easily use Serpro Datavalid API",
    "license": "MIT",
    "keywords": ["laravel", "serpro", "datavalid"],
    "authors": [
        {
            "name": "Lucas Giovanny",
            "email": "lucasgiovanny@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "laravel/pint": "^1.7",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "LucasGiovanny\\SerproDataValid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucasGiovanny\\SerproDataValid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucasGiovanny\\SerproDataValid\\SerproDataValidServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
